#include <iostream>
using namespace std;

int factorial(int n) {
    int result = 1;
    for (int i = 2; i <= n; ++i) {
        result *= i;
    }
    return result;
}

int main() {
    int M;
    cin >> M;

    for (int i = 0; i < M; ++i) {
        int n;
        cin >> n;
        cout << factorial(n) << endl;
    }

    return 0;
}
