#include <iostream>
#include <string>

int main() {
    int N, M;
    std::cin >> N >> M;

    std::string strN = std::to_string(N);
    std::string strM = std::to_string(M);

    int count = 0;
    for (size_t i = 0; i <= strM.length() - strN.length(); ++i) {
        if (strM.substr(i, strN.length()) == strN) {
            count++;
        }
    }

    std::cout << count << std::endl;

    return 0;
}
