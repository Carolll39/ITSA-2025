#include <iostream>
using namespace std;

int getDayOfWeek(int y, int m, int d) {
    if (m == 1 || m == 2) {
        m += 12;
        y -= 1;
    }
    int K = y % 100;
    int J = y / 100;
    int h = (d + 13 * (m + 1) / 5 + K + K / 4 + J / 4 + 5 * J) % 7;

    int day = ((h + 6) % 7) + 1;
    return day;
}

int main() {
    int T;
    cin >> T;
    while (T--) {
        int Y, M, D;
        cin >> Y >> M >> D;
        cout << getDayOfWeek(Y, M, D) << endl;
    }
    return 0;
}
